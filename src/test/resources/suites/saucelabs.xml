<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="SauceLabs Test Suite">
    <test name="Edge on Windows 10">
        <parameter name="platform" value="Windows 10"/>
        <parameter name="browserName" value="MicrosoftEdge"/>
        <parameter name="browserVersion" value="latest"/>
        <classes>
            <class name="com.coherentsolutions.training.automation.web.sirbu.LoginLogoutTest"/>
        </classes>
    </test>
    <test name="Firefox 100 on Windows 8.1">
        <parameter name="platform" value="Windows 8.1"/>
        <parameter name="browserName" value="firefox"/>
        <parameter name="browserVersion" value="100"/>
        <classes>
            <class name="com.coherentsolutions.training.automation.web.sirbu.LoginLogoutTest"/>
        </classes>
    </test>
    <test name="Chrome 100 on Linux">
        <parameter name="platform" value="Linux"/>
        <parameter name="browserName" value="chrome"/>
        <parameter name="browserVersion" value="100"/>
        <classes>
            <class name="com.coherentsolutions.training.automation.web.sirbu.LoginLogoutTest"/>
        </classes>
    </test>
</suite>